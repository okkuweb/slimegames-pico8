pico-8 cartridge // http://www.pico-8.com
version 16
__lua__
-- slimegames
-- pico-8 edition

function _init ()
	cls()
	--< player positions >--
	size = 16
	-- p1 init
	p1 = {}
	p1.index = 0
	p1.timeout = false
	p1.timelen = 10
	p1.time = 100
	p1.xinit = 20
	p1.yinit = 80
	p1.xmid = size/2
	p1.ymid = size-1
	p1.x = p1.xinit
	p1.y = p1.yinit
	-- p2 init
	p2 = {}
	p2.index = 1
	p2.timeout = false
	p2.timelen = 10
	p2.time = 100
	p2.xinit = 90
	p2.yinit = 80
	p2.xmid = size/2
	p2.ymid = size-1
	p2.x = p2.xinit
	p2.y = p2.yinit
	
	--< global movement >--
	speed = 4
	jgravityinit = 0.1
	jspeedinit = 2
	
	--< player movement >--
	-- p1
	p1.jspeed = jspeedinit
	p1.jgravity = jgravityinit
	p1.jstate = false
	-- p2
	p2.jgravity = jgravityinit
	p2.jspeed = jspeedinit
	p2.jstate = false

	--< ball >--
	b = {}
	b.x = 27
	b.y = 50
	b.xspeed = 0
	b.yspeed = 0
	b.hitspeed = 2
	b.gravity = 0.05
	b.radius = 2
	b.color = 6

	--< helper variables >--
	occurrence = 0
end

function _update60()
	--< player 1 movement >--
	move(p1)
	jump(p1)
	
	--< player 2 movement >--
	move(p2)
	jump(p2)
	
	--< ball movement >--
	ball_move()
	wall_hit()
	player_hit(p1)
	player_hit(p2)
end

function move(p)
	if btn(1,p.index) then
		p.x += 1
	end
	if btn(0,p.index) then
		p.x -= 1
	end
end

function jump(p)
	-- initiate jump
	if btn(2,p.index) and not p.jstate then
		p.jstate = true
 	p.y -= p.jspeed
	end
	
	-- produce jump motion
	if p.y < p.yinit then
		p.jstate = true
		p.jspeed -= p.jgravity
 	p.y -= p.jspeed
	else
		-- reset jump parameters
		p.jstate = false
		p.jspeed = jspeedinit
		p.jgravity = jgravityinit
		-- horrible fix for the
		-- occasional floor clip
		p.y = p.yinit + 0.01
	end
end

function ball_move()
	-- ball gravity
	b.yspeed += b.gravity
	-- ball movement
	b.y += b.yspeed
	b.x += b.xspeed
end

function wall_hit()
	-- if hit left wall
	if b.x-b.radius < 0 and
			 b.xspeed < 0 then
		b.xspeed = -b.xspeed
	end
	-- if hit right wall
	if b.x+b.radius > 128 and
	   b.xspeed > 0 then
	 b.xspeed = -b.xspeed
	end
	
	if b.y+b.radius > 95 and
				b.yspeed > 0
				then
		b.angle = atan2(b.xspeed,b.yspeed)
		b.yspeed = -sin(b.angle)*b.hitspeed
	end
end

function player_hit(p)
	-- ball distance from p1
	local distance = (
		sqrt(
			(p.x+p.xmid-b.x)^2+
			(p.y+p.ymid-b.y)^2
		)
	)
	
	p.time += 1
	
	if p.timeout
				and p.time > p.timelen
				then
		p.timeout = false
	end
	
	-- if hit
	if b.y < p.y + size
				and
				distance<=size/2+b.radius
				and
				not p.timeout
				then
		occurrence += 1
 	b.angle = atan2(
 												p.x+p.xmid-b.x,
 												p.y+p.ymid-b.y
 											)
 	x_side = cos(b.angle)*b.hitspeed
 	y_side = sin(b.angle)*b.hitspeed
 	
 	b.xspeed = -x_side
 	b.yspeed = -y_side

		p.timeout = true
		p.time = 0
	end
end

function _draw()
 cls()
 map(0,0)
 -- p1 sprite
 sspr(
 	0,8,
 	size,size,
 	p1.x,p1.y
 )
 
 -- player 2 sprite
 sspr(
 	16,8,
 	size*2,size*2,
 	p2.x,p2.y
 )
	-- ball sprite
	circfill(
		b.x,b.y,b.radius,b.color
	)

	-- helpers
 pset(
 	p1.x+p1.xmid,p1.y+p1.ymid,2
 )
 circ(
 	p1.x+p1.xmid,p1.y+p1.ymid,size/2+1,14
 )
 line(
 	p1.x+p1.xmid,p1.y+p1.ymid,
 	b.x,b.y,
 	3
 )
 print(p1.y+size)
 print(b.y)
end
